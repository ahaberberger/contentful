{% extends 'ahaberbergerContentfulBundle::layout.html.twig' %}
{% block content %}
<ul>
    {% for entry in entries %}
    <li class="first">
        <h3><a href="blog.html">{{ entry.fields.title }}</a></h3>
        <span>Posted : {{ entry.fields.date }}</span> <a href="blog.html"><img src="images/girl.jpg" alt=""></a>
        <p>
            {{ entry.fields.text }}
        </p>
    </li>
    {% endfor %}
</ul>
{% endblock %}

